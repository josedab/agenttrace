# AgentTrace Production Environment Variables
# Copy this file to .env and fill in the values

# ============================================
# Database Credentials (Required)
# ============================================
POSTGRES_USER=agenttrace
POSTGRES_PASSWORD=your-secure-postgres-password
POSTGRES_DB=agenttrace

CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=your-secure-clickhouse-password
CLICKHOUSE_DB=agenttrace

REDIS_PASSWORD=your-secure-redis-password

MINIO_ROOT_USER=agenttrace
MINIO_ROOT_PASSWORD=your-secure-minio-password

# ============================================
# Security Keys (Required)
# ============================================
# Generate with: openssl rand -base64 32
JWT_SECRET=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key

# ============================================
# NextAuth Configuration (Required)
# ============================================
NEXTAUTH_URL=https://your-domain.com
NEXTAUTH_SECRET=your-nextauth-secret

# ============================================
# OAuth Providers (Optional)
# ============================================
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# ============================================
# API Configuration (Optional)
# ============================================
API_HOST=0.0.0.0
API_PORT=8080
NEXT_PUBLIC_API_URL=http://api:8080

# ============================================
# External Services (Optional)
# ============================================
# Required for LLM-as-Judge evaluators
OPENAI_API_KEY=

# ============================================
# Deployment (Optional)
# ============================================
VERSION=latest
WEB_PORT=3000
