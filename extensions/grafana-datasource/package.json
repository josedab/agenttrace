  },
  "devDependencies": {
    "@grafana/data": "^10.0.0",
    "@grafana/runtime": "^10.0.0",
    "@grafana/toolkit": "^10.0.0",
    "@grafana/ui": "^10.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.28",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
